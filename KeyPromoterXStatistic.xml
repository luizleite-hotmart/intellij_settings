<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Analyze with SonarLint">
        <Item>
          <IdeaActionID>SonarLint.AnalyzeFiles</IdeaActionID>
          <ShortCut>'Ctrl+Shift+S'</ShortCut>
          <Description>Analyze with SonarLint</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Alt+Shift+Left'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cancel Running Statements">
        <Item>
          <IdeaActionID>Console.Jdbc.Terminate</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Cancel Running Statements</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'SendEmailIntegrationTest.sendEmailSoldOutProduct'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'SendEmailIntegrationTest.sendEmailSoldOutProduct'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find">
        <Item>
          <IdeaActionID>ActivateFindToolWindow</IdeaActionID>
          <ShortCut>'Alt+3'</ShortCut>
          <Description>Find</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Structure...">
        <Item>
          <IdeaActionID>ShowProjectStructureSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+S'</ShortCut>
          <Description>Project Structure...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'Shift+Enter'</ShortCut>
          <Description>Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Commit">
        <Item>
          <IdeaActionID>Console.Transaction.Commit</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+Enter'</ShortCut>
          <Description>Commit</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>